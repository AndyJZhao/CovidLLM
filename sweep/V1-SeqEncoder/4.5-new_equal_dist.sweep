# sw sweep/V1-SeqEncoder/4.0-Dynamic.sweep
command:
  - ${interpreter}
  - src/scripts/run_covid_llm_sft.py
  - ${args_no_hyphens}
method: grid # Bayesian
metric:
  goal: minimize
  name: val_wmse

name: V4.5_new_equal_dist
parameters:
  use_deepspeed:
    values:
      - false

  use_wandb:
    values:
      - true

  total_steps:
    values:
      - 1001

  best_eval_metrics:
    values:
      - val_wmse

  data_aug:
    values:
      - true
      - false

  data_file:
    values:
      - processed_v4_5.pkl
      - processed_equal_dist.pkl
